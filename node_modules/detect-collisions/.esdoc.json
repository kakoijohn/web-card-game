{
    "source": "./source",
    "destination": "./docs",
    "includes": ["\\.js$", "\\.mjs$"],
    "plugins": [{
        "name": "esdoc-standard-plugin",
        "option": {
            "accessor": {
                "access": ["public", "protected"],
                "autoPrivate": true
            },
            "lint": { "enable": true },
            "typeInference": { "enable": true }
        }
    }]
}